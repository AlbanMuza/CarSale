@model CarSale.Models.Car


@{
    ViewData["Title"] = "FileUploadPage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>FileUploadPage</h4>

<p>@ViewBag.Message</p>

<form method="post" enctype="multipart/form-data" asp-controller="Cars" asp-action="FileUploadPage">
    
<div>
    <div>
        <input type="file" name="files" multiple/>
        <input type="hidden" asp-for="Id"/>
    </div>
    <div>
        <input type="submit" value="upload" />
    </div>
</div>





</form>
